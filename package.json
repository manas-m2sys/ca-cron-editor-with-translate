{
  "name": "cron-editor-app",
  "description": "A cron expression generator to be used in Angular applications",
  "author": {
    "name": "Manas Datta",
    "email": "manas@m2sys.com"
  },
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/manas-m2sys/cloudapper-cron-editor"
  },
  "bugs": {
    "url": "https://github.com/manas-m2sys/cloudapper-cron-editor/issues"
  },
  "homepage": "https://github.com/manas-m2sys/cloudapper-cron-editor",
  "keywords": [
    "angular",
    "cron",
    "cron-expression",
    "angular-cli",
    "quartz",
    "ngx-translate"
  ],
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build cloudapper-cron-editor",
    "build": "npm run build_lib && ng build --configuration production",
    "start": "npm run build_lib && ng serve",
    "copy-license": "node -e \"require('fs').copyFileSync('LICENSE.md', 'dist/cloudapper-cron-editor/LICENSE.md')\"",
    "copy-readme": "node -e \"require('fs').copyFileSync('README.md', 'dist/cloudapper-cron-editor/README.md')\"",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "pack_lib": "cd dist/cloudapper-cron-editor && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run pack_lib"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~16.2.0",
    "@angular/common": "~16.2.0",
    "@angular/compiler": "~16.2.0",
    "@angular/core": "~16.2.0",
    "@angular/forms": "~16.2.0",
    "@angular/platform-browser": "~16.2.0",
    "@angular/platform-browser-dynamic": "~16.2.0",
    "@angular/router": "~16.2.0",
    "@ngx-translate/core": "^15.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~7.8.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "~16.2.0",
    "@angular/language-service": "~16.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~16.11.7",
    "bootstrap": "~5.3.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.7.1",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "tsickle": ">=0.29.0",
    "tslib": "^2.3.1",
    "tslint": "~6.1.3",
    "typescript": "~5.1.3"
  }
}
